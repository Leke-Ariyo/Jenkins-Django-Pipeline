[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=jenkins
Group=nginx
WorkingDirectory=$WORKSPACE
ExecStart=$WORKSPACE/env/bin/gunicorn --workers 3 --log-level debug --error-logfile $WORKSPACE/error.log --bind unix:$WORKSPACE/app.sock myproject.wsgi:application

[Install]
WantedBy=multi-user.target